{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "functions": {
    "api/bot.ts": {
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "env": {
    "NODE_OPTIONS": "--dns-result-order=ipv4first"
  }
}