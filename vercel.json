{
  "version": 2,
  "functions": {
    "api/bot.ts": {
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "env": {
    "NODE_OPTIONS": "--dns-result-order=ipv4first"
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    }
  ]
}